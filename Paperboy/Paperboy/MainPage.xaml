	</TabbedPage.ToolbarItems>
	
	<TabbedPage.Children>
		<pages:Page1 Title="Page 1"></pages:Page1>
		<pages:TestingPage Title="Testing"></pages:TestingPage>
		<pages:ListViewPage1 Title="List View"></pages:ListViewPage1>
	</TabbedPage.Children>
	
</TabbedPage>
